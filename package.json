{
  "name": "yardsale",
  "version": "1.0.0",
  "description": "this is a ecommerce software",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon app.ts",
    "build": "tsc",
    "start": "node ./build/app.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelsonalejandro/yardsale.git"
  },
  "keywords": [
    "yard",
    "sale",
    "ecommerce",
    "typescript",
    "javascript"
  ],
