  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nelsonalejandro/yardsale/issues"
  },
  "homepage": "https://github.com/nelsonalejandro/yardsale#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.11",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/pg": "^8.6.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "sequelize-cli": "^6.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
